{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "/usr/include",
                "/usr/local/cuda-12.2/include",
                "/usr/local/cuda-12.2/targets/x86_64-linux/include",
                "${workspaceFolder}/cutlass/include",
                "${workspaceFolder}/cutlass/tools/util/include",
                "${workspaceFolder}/src/**"
            ],
            "defines": [
                "GOOGLE_CUDA",
                "CUTLASS_ARCH_MMA_SM90_SUPPORTED",
                "__CUDACC__",
                "__CUDA_ARCH__=900"
            ],
            "compilerPath": "/usr/local/cuda-12.2/bin/nvcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "${default}",
            "browse": {
                "databaseFilename": "${workspaceFolder}/.vscode/vc.db",
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}