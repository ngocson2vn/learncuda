  "version": "0.2.0",
  "configurations": [
    {
      "name": "cuda_tma",
      "type": "cuda-gdb",
      "request": "launch",
      "program": "${workspaceFolder}/src/cuda_tma/main2",
      "cwd": "${workspaceFolder}/src/cuda_tma",
      "args": [],
      "stopAtEntry": true,
      "environment": [
          {
              "name": "TF_CPP_MAX_VLOG_LEVEL",
              "value": "0"
          },
          {
              "name": "CUDA_VISIBLE_DEVICES",
              "value": "0"
          }
      ],
      "miDebuggerArgs": "-x '${workspaceFolder}/gdb.conf'"
    }
  ]
}